{
	"name": "akvj",
	"version": "0.0.0",
	"description": "A Vite project for AKVJ",
	"main": "main.js",
	"type": "module",
	"private": false,
	"scripts": {
		"dev": "vite",
		"build": "vite build --emptyOutDir",
		"preview": "vite preview",
		"format:stylelint": "stylelint \"**/*.css\" --fix",
		"format:prettier": "prettier --write .",
		"fix-install": "npm prune && npm install",
		"fix-quick": "npm prune && npm audit fix --force && npm install",
		"fix-upgrade": "npm prune && npm install && npm update && npm audit fix --force",
		"fix-deep": "rm -rf node_modules package-lock.json && npm prune && npm install && npm update --force && npm audit fix --force && npm ci && npm prune"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KristofferKarlAxelEkstrand/AKVJ"
	},
	"author": {
		"name": "Kristoffer Ekstrand",
		"email": "hej@kristofferekstrand.se",
		"url": "https://kristofferekstrand.se"
	},
	"license": "MIT",
	"devDependencies": {
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "3.2.5",
		"stylelint": "^16.6.1",
		"stylelint-config-recommended": "^14.0.0",
		"stylelint-order": "^6.0.4",
		"vite": "^5.2.0"
	}
}
